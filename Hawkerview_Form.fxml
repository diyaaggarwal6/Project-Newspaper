         <font>
            <Font name="Cambria Bold" size="40.0" />
         </font>
      </Text>
      <Button fx:id="btnshowall" layoutX="240.0" layoutY="136.0" mnemonicParsing="false" onAction="#doshowall" style="-fx-background-color: #302455;" text="Show All" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="20.0" />
         </font>
      </Button>
      <TableView fx:id="tblview" layoutX="7.0" layoutY="195.0" prefHeight="355.0" prefWidth="585.0" />
      <ComboBox fx:id="comboarea" layoutX="178.0" layoutY="85.0" onAction="#docomboarea" prefWidth="150.0" visibleRowCount="30" />
      <Label layoutX="115.0" layoutY="89.0" text="Areas" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="btnfetcharea" layoutX="358.0" layoutY="82.0" mnemonicParsing="false" onAction="#dofetcharea" style="-fx-background-color: #302455;" text="Fetch Areas" textFill="WHITE">
